def insertion_sort(arr):
	for i in range(1, len(arr)):
		key = arr[i]
		j = i - 1
		while j >= 0 and key < arr[j]:
			arr[j + 1] = arr[j]
			j -= 1
		arr[j + 1] = key
	return arr

print(insertion_sort([51, - 4, 3, 2, 1]))

def palindrome(s):
	if len(s) <= 1:
		return True
	for i in range(len(s) // 2):
		if s[i] != s[len(s) - i - 1]:
			return False
	return True